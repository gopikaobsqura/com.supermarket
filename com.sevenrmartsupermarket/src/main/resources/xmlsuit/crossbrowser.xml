<suite name="cross browser execution" thread-count="5" parallel="tests">
	<listeners>
		<listener
			class-name="com.sevenrmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="Execution in Chrome browser">
	<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>
	<test name="Execution in  firefox browser">
	<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>
	<test name="Execution in  edge browser">
	<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest">
			</class>
		</classes>
	</test>
	</suite>